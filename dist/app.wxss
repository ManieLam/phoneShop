@charset "UTF-8";
page {
  font-family: -apple-system-font, "Helvetica Neue", sans-serif;
}

.col_b2 {
  color: #b2b2b2;
}

.col_35 {
  color: #353535;
}

.test {
  background: #999;
  width: 100%;
}

.view-header {
  height: 550rpx;
  width: 100%;
  position: relative;
  border-bottom: 1rpx dotted #ddd;
}

.view-header image {
  width: 100%;
  height: 350rpx;
}

.header_float {
  position: absolute;
  bottom: 18px;
  left: 0;
  right: 0;
  margin: 0 auto;
  width: 600rpx;
  height: 300rpx;
  background: #fff;
  color: #231815;
  border: 1rpx dotted #ddd;
  padding: 0 10px 10px;
}

.header_float image {
  vertical-align: middle;
  margin: 0 10px;
}

.float_hd {
  padding: 10px 0;
  font-weight: 600;
  font-size: 18pt;
  border-bottom: 1rpx dotted #ddd;
  vertical-align: middle;
}

.float_hd image {
  width: 100rpx;
  height: 100rpx;
  min-width: 100rpx;
  border-radius: 50%;
  border: 1rpx solid #ddd;
}

.float_ft .ft_line {
  padding: 5px 0;
  line-height: 1.2;
  margin: 0;
  border: 0;
}

.float_ft image {
  width: 30rpx;
  min-width: 30rpx;
  height: 36rpx;
}

.float_ft text {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.con_section {
  border-bottom: 10px solid #f7f7f7;
  background: #fff;
}

.adds_bar {
  width: 100%;
  height: 200rpx;
}

.adds_bar image {
  width: 100%;
  height: 100%;
}

/*列表*/
.list_content {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}

.list_hd {
  font-weight: 600;
  padding: 5px 15px;
}

.list_bd {
  padding: 5px 0;
  margin: 0 15px;
}

.list_hd,
.list_bd {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  line-height: 1.8;
  border-bottom: 1rpx dotted #ddd;
}

.list_hd .left,
.list_bd .left {
  width: 30rpx;
  min-width: 30rpx;
  vertical-align: middle;
  padding-right: 5px;
}

.list_hd .left .icon,
.list_bd .left .icon {
  margin: 0;
}

.list_hd .center,
.list_bd .center {
  -webkit-box-flex: 1;
  -webkit-flex-grow: 1;
          flex-grow: 1;
  padding-right: 5px;
}

.list_bd:last-child {
  border: 0;
}

.lists-section .list_bd {
  background: #fff;
  margin: 0;
  padding: 30rpx;
  border-bottom: 10px solid #f7f7f7;
}

.lists-section .right {
  width: 200rpx;
  min-width: 200rpx;
  height: 150rpx;
}

.lists-section .right image {
  width: 100%;
  height: 100%;
}

.list_content .content__title {
  line-height: 1.6;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.list_content .content__desc {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  font-size: 12px;
  color: #9c9c9c;
}

.lists-section__group {
  border: 10px solid #f7f7f7;
}

.lists-group {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  position: relative;
  padding: 10px 15px;
  background: #fff;
  line-height: 80rpx;
}

.lists-group:before {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1rpx solid #D9D9D9;
  color: #D9D9D9;
  left: 15px;
}

.lists-group:first-child:before {
  display: none;
}

.lists-group_active {
  background-color: #ECECEC;
}

.lists-group_primary {
  -webkit-box-align: start;
  -webkit-align-items: flex-start;
  align-items: flex-start;
}

.lists-group__bd {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1 1 0%;
}

.lists-group__ft {
  text-align: right;
  color: #999999;
}

.lists-group__cell {
  border-bottom: 10px solid #f7f7f7;
}

.lists-group__cell .list_bd {
  margin: 0;
  padding: 20rpx;
}

.lists-group__cell .right {
  width: 200rpx;
  min-width: 200rpx;
  height: 150rpx;
}

.lists-group__cell .right image {
  width: 100%;
  height: 100%;
}

/*显示隐藏*/
.hidden {
  display: none;
}

.show {
  display: block;
}

.m-single {
  padding: 10px 0;
}

.m-single::after {
  content: "";
  display: table;
  clear: both;
}

.single-cell {
  float: left;
  box-sizing: border-box;
  width: 50%;
  padding-right: 2.5px;
  padding-left: 10px;
  margin-bottom: 5px;
}

.single-cell:nth-child(2n) {
  padding-right: 10px;
  padding-left: 2.5px;
}

.single-block {
  background: #fff;
}

.single-block .block_hd {
  height: 340rpx;
  width: 100%;
}

.single-block image {
  width: 100%;
  height: 100%;
}

.single-block .block_bd {
  padding: 8px;
  line-height: 1.6;
}

.single-block .bd_title {
  font-size: 14px;
}

.single-block .bd_desc {
  padding-top: 5px;
  color: #ff7800;
  font-weight: 600;
}

.single-header {
  /*height: 980rpx;*/
  border-bottom: 10px solid #f7f7f7;
}

.single-body {
  padding-bottom: 200rpx;
}

.single-footer {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  width: 100%;
  box-sizing: border-box;
  padding: 5px 30px;
  background: #fff;
  border-top: 1rpx solid #f7f7f7;
}

.single-footer .ft__nav {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  text-align: center;
}

.single-footer .ft__nav text {
  font-size: 14px;
}

.icon {
  width: 30rpx;
  height: 30rpx;
  vertical-align: middle;
  margin: 0 5px;
}

.icon-arrow_right {
  background: url(data:image/svg+xml;base64,);
  background-size: 100% 100%;
}

.icon-arrow_down {
  background: url(data:image/svg+xml;base64,);
  background-size: 100% 100%;
}

.icon_users {
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
}

.icon_phone_lar {
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
}

.icon_list {
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
}

.icon_photos {
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
}

.triangle_under {
  width: 0px;
  height: 0px;
  border-color: #888888;
  border-top: 20rpx solid;
  border-right: 13rpx solid transparent;
  border-left: 13rpx solid transparent;
  /*border-bottom: 1rpx solid transparent;*/
}

.triangle_under_choose {
  border-top: 0;
  border-bottom: 20rpx solid #b2b2b2;
  /*transform: rotate(180deg);
    -webkit-transform: rotate(180deg);*/
}

/*分界线*/
.hint {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  font-size: 13px;
  color: #888;
  margin-top: 15px;
  margin-bottom: 15px;
}

.hint__line {
  width: 60px;
  height: 1px;
  border-bottom: 1rpx solid #ccc;
  margin-left: 5px;
  margin-right: 5px;
}

/*navbar*/
#navbar {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  position: fixed;
  width: 100%;
  height: 90rpx;
  line-height: 28rpx;
  padding: 0 15px;
  box-sizing: border-box;
  background: #fff;
  border-bottom: 1rpx solid #ddd;
}

#navbar .item {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1 1 0%;
  text-align: center;
  font-weight: 500;
  color: #cecece;
}

#navbar .item.active {
  color: #333333;
  position: relative;
}

#navbar .item.active:after {
  content: '';
  position: absolute;
  display: block;
  left: 0;
  right: 0;
  bottom: 0;
  margin: 0 auto;
  height: 8rpx;
  width: 70%;
  background-color: #2f6ed5;
}

/*scroll*/
.swiper-box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  box-sizing: border-box;
  width: 100%;
  height: 210rpx;
  padding: 10px 0;
  overflow: hidden;
}

.scroll-box {
  white-space: nowrap;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: nowrap;
          flex-wrap: nowrap;
  overflow-x: scroll;
  width: 100%;
  height: 210rpx;
  padding: 10px 0;
  /*取消移动条*/
}

.scroll-box image {
  width: 28%;
  height: 100%;
  margin: 0 5rpx;
}

.scroll-content {
  display: inline-block;
  margin: 0 5rpx;
  height: 100%;
  width: 28%;
}
